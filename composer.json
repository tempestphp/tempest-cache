    "description": "The PHP framework that gets out of your way.",
    "require": {
        "php": "^8.4",
        "psr/cache": "^3.0",
        "symfony/cache": "^7.3",
        "tempest/core": "1.6.0",
        "tempest/clock": "1.6.0",
        "tempest/kv-store": "1.6.0",
        "tempest/container": "1.6.0"
    },
    "require-dev": {
        "tempest/clock": "1.6.0"
    },
    "autoload": {
        "psr-4": {
